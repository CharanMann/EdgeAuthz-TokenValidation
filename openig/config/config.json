{
  "properties": {
    "frConfigs": {
      "$location": "${fileToUrl(openig.configDirectory)}/frProps.json"
    }
  },
  "handler": {
    "name": "_router",
    "type": "Router",
    "audit": "global",
    "capture": "all"
  },
  "heap": [
    {
      "name": "capture",
      "type": "CaptureDecorator",
      "config": {
        "captureEntity": true,
        "_captureContext": true
      }
    }
  ]
}